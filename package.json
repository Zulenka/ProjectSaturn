{
  "name": "violentmonkey",
  "description": "Violentmonkey",
  "version": "2.35.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "gulp dev",
    "prebuild": "yarn ci",
    "build": "cross-env NODE_ENV=production gulp build",
    "build:raw": "cross-env NODE_ENV=production gulp build",
    "build:chrome": "cross-env TARGET_BROWSER=chrome run-s build:raw package:target",
    "build:chrome-mv3": "cross-env TARGET_BROWSER=chrome TARGET_MANIFEST=mv3 run-s build:raw package:target",
    "build:firefox": "cross-env TARGET_BROWSER=firefox run-s build:raw package:target",
    "build:opera": "cross-env TARGET_BROWSER=opera run-s build:raw package:target",
    "build:opera-mv3": "cross-env TARGET_BROWSER=opera TARGET_MANIFEST=mv3 run-s build:raw package:target",
    "build:all": "run-s build:chrome build:firefox build:opera",
    "build:all:mv3": "run-s build:chrome-mv3 build:opera-mv3",
    "build:selfHosted": "cross-env TARGET=selfHosted TARGET_BROWSER=firefox BETA=1 run-s build:raw",
    "package:target": "node scripts/export-dist.mjs",
    "check:mv3:guards": "node scripts/check-mv3-guards.js",
    "check:mv3:blocking-gates": "node scripts/check-mv3-blocking-gates.js",
    "check:mv3:sw-safety": "node scripts/check-mv3-sw-safety.js",
    "check:mv3:runtime-contracts": "node scripts/check-mv3-runtime-contracts.js",
    "check:mv3:sync-provider-contracts": "node scripts/check-mv3-sync-provider-contracts.js",
    "check:mv3:release-contract": "node scripts/check-mv3-release-contract.js",
    "report:mv3:triage": "bash scripts/mv3-regression-report.sh",
    "rollout:mv3:canary": "bash scripts/mv3-rollout-gate.sh canary",
    "rollout:mv3:beta": "bash scripts/mv3-rollout-gate.sh beta",
    "rollout:mv3:stable": "bash scripts/mv3-rollout-gate.sh stable",
    "verify:artifacts:mv3": "node scripts/verify-build-artifacts.js mv3",
    "verify:artifacts:dual": "node scripts/verify-build-artifacts.js dual",
    "analyze": "cross-env RUN_ENV=analyze run-s build",
    "i18n": "gulp i18n",
    "copyI18n": "gulp copyI18n",
    "lint": "run-s lint:js lint:yml",
    "lint:js": "eslint --ext .js,.vue . --cache",
    "lint:yml": "gulp check",
    "test": "cross-env BABEL_ENV=test TEST=test jest test",
    "smoke:mv3:unit": "cross-env BABEL_ENV=test TEST=test jest --runInBand test/scripts/manifest-helper.test.js test/scripts/action-helper.test.js test/scripts/verify-build-artifacts.test.js test/common/commands.test.js test/common/webdav-xml.test.js test/background/tabs.test.js test/background/sync-base.test.js test/background/tab-redirector.test.js test/background/popup-tracker.test.js test/background/requests-core.test.js test/background/requests-offscreen.test.js test/background/icon-menu.test.js test/background/diagnostics.test.js test/background/storage.test.js test/background/sync-providers.test.js test/background/clipboard.test.js test/background/update.test.js test/background/export-import.test.js test/background/preinject.test.js test/background/preinject-csp.test.js",
    "smoke:mv3:test": "run-s check:mv3:guards check:mv3:blocking-gates check:mv3:sw-safety check:mv3:runtime-contracts check:mv3:sync-provider-contracts check:mv3:release-contract smoke:mv3:unit",
    "smoke:mv3": "run-s smoke:mv3:test build:all:mv3 verify:artifacts:mv3",
    "ci": "run-p lint test",
    "bumpVersion": "gulp bump",
    "bump": "run-s ci \"bumpVersion --commit\"",
    "preversion": "run-s ci \"bumpVersion --reset\""
  },
  "devDependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/helper-plugin-utils": "^7.18.6",
    "@babel/plugin-syntax-function-bind": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@iconify-json/mdi": "^1.2.3",
    "@types/chrome": "^0.0.208",
    "@types/firefox-webext-browser": "94.0.1",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@violentmonkey/types": "^0.3.1",
    "@vue/compiler-sfc": "^3.5.25",
    "@vue/eslint-config-typescript": "^13.0.0",
    "amo-upload": "^0.5.5",
    "autoprefixer": "^10.4.15",
    "babel-jest": "^29.6.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-modern-regexp": "^0.0.6",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "del": "^6.1.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^2.0.0",
    "postcss": "^8.4.28",
    "postcss-calc": "^9.0.1",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.4",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^3.0.2",
    "sharp": "^0.32.6",
    "style-loader": "^3.3.3",
    "svg-sprite-loader": "^6.0.11",
    "terser-webpack-plugin": "^5.3.9",
    "through2": "^4.0.2",
    "typescript": "^5.1.6",
    "unplugin-icons": "^22.5.0",
    "vinyl": "^2.2.1",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.29",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "author": "Gerald <i@gerald.top>",
  "repository": {
    "type": "git",
    "url": "https://github.com/violentmonkey/violentmonkey.git"
  },
  "bugs": {
    "url": "https://github.com/violentmonkey/violentmonkey/issues"
  },
  "homepage": "https://github.com/violentmonkey/violentmonkey",
  "license": "MIT",
  "dependencies": {
    "@violentmonkey/shortcut": "^1.4.4",
    "@zip.js/zip.js": "2.4.4",
    "codemirror": "^5.65.20",
    "tldts": "^7.0.19",
    "vue": "^3.5.25",
    "vueleton": "^2.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/mock/index.js"
    ],
    "testEnvironment": "./test/mock/env.js"
  }
}
